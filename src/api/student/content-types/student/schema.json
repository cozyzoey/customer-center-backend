{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "schoolName": {
      "type": "string",
      "minLength": 2
    },
    "schoolYear": {
      "type": "integer",
      "max": 3,
      "min": 1
    },
    "schoolClass": {
      "type": "integer",
      "min": 1,
      "max": 99
    },
    "studentNumber": {
      "type": "integer",
      "min": 1,
      "max": 99
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "phoneNumber": {
      "type": "string",
      "regex": "^01([0|1|6|7|8|9])-?([0-9]{3,4})-?([0-9]{4})$"
    },
    "parentName": {
      "type": "string",
      "minLength": 2
    },
    "parentPhoneNumber": {
      "type": "string",
      "regex": "^01([0|1|6|7|8|9])-?([0-9]{3,4})-?([0-9]{4})$"
    },
    "dataCollectionSession": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::data-collection-session.data-collection-session"
    }
  }
}
